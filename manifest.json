{
   "manifest_version": 3,
   "name": "Gerador de Dados",
   "description": "Gerar CPF, RG, PIS, CNPJ, Inscrição Estadual, CEP e Agências Bancárias. Útil para DEVs e QAs realizarem testes de sistemas.",
   "version": "1.1",
   "icons": {
      "16": "assets/images/icon-16.png",
      "32": "assets/images/icon-32.png",
      "48": "assets/images/icon-48.png",
      "128": "assets/images/icon-128.png"
   },
   "action": {
      "default_icon": "assets/images/icon-32.png",
      "default_popup": "popup.html",
      "default_title": "Gerador de Dados"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "offline_enabled": true,
   "permissions": [
      "storage",
      "background",
      "contextMenus",
      "scripting"
   ],
   "host_permissions": [
      "*://*/*"
   ],
   "author": "Marcelo Lourenço"
}